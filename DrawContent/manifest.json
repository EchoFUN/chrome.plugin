{
   "manifest_version": 2,
   "name": "DrawContent Extension for Chrome",
   "version": "0.0.1",
   "description": "Get the content from the current view of the webside.",
   "content_scripts": [
      {
         "matches": ["<all_urls>"],
         "js": ["app.js"],
         "css": ["app.css"],
         "run_at": "document_start",
         "all_frames": false
      }
   ]
}
